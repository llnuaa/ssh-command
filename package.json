{
	"name": "ssh-command",
	"displayName": "ssh-command",
	"description": "SSH and Command for Visual Studio Code",
	"version": "0.0.1",
	"publisher": "longbl",
	"homepage": "https://github.com/llnuaa/ssh-command#readme",
	"bugs": {
		"url": "https://github.com/llnuaa/ssh-command/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/llnuaa/ssh-command"
	},
	"license": "MIT",
	"author": "longbl",
	"icon": "resources/ssh-command-128.png",
	"categories": [
		"Extension Packs",
		"Other"
	],
	"keywords": [
		"longbl",
		"json",
		"html",
		"css",
		"javascript",
		"nodejs",
		"github",
		"gitlab",
		"git",
		"c",
		"C++",
		"web",
		"js",
		"npm",
		"python",
		"java",
		"Ruby",
		"webpack",
		"php",
		"mysql",
		"sql",
		"apache",
		"nginx",
		"iis",
		"wordpress",
		"hexo",
		"tencent",
		"linux",
		"window",
		"mac",
		"vs",
		"google",
		"markdown",
		"xml",
		"run",
		"commands",
		"view",
		"diff",
		"tree",
		"compare"
	],
	"engines": {
		"vscode": "^1.46.0"
	},
	"enableProposedApi": true,
	"main": "./out/extension.js",
	"activationEvents": [
		"onView:sshServerTree",
		"onLanguage:json",
		"onCommand:sshServerTree.openSshServer"
	],
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "ssh-command-views",
					"title": "ssh-command",
					"icon": "resources/lightning.png"
				}
			]
		},
		"views": {
			"ssh-command-views": [
				{
					"id": "sshServerTree",
					"name": "ssh server"
				},
				{
					"id": "jsonOutline1",
					"name": "quick command"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "sshServerTree.refresh",
					"when": "view == sshServerTree || view == jsonOutline1",
					"group": "navigation"
				},
				{
					"command": "sshServerTree.edit",
					"when": "view == sshServerTree || view == jsonOutline1",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
                    "command": "sshServerTree.openSshServer",
                    "when": "view == sshServerTree",
                    "group": "sshServer@1"
                }
			]
		},
		"commands": [
			{
				"command": "sshServerTree.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "sshServerTree.edit",
				"title": "Edit SSH server and Command Config",
				"icon": {
					"light": "resources/light/edit.svg",
					"dark": "resources/dark/edit.svg"
				}
			},
            {
                "command": "sshServerTree.openSshServer",
                "title": "Opem SSH Server"
            }
		],
		"languages": [
			{
				"id": "json",
				"extensions": [
					".json"
				],
				"aliases": [
					"Json"
				]
			}
		],
		"configuration": [
			{
				"type": "object",
				"title": "SSH-Command",
				"properties": {
					"SSH-Command.author.longbl": {
						"type": "string",
						"default": "Welcome to ssh-command.",
						"description": "SSH and Command for Visual Studio Code"
					},
					"SSH-Command.config.path": {
						"type": "string",
						"default": "/home/longbl/ssh-command.json",
						"description": "The real path about ssh server and command config json file"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.45.0",
		"typescript": "^3.9.4",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0"
	},
	"dependencies": {
		"jsonc-parser": "^0.4.2"
	}
}